<Application x:Class="TagShelfLocator.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TagShelfLocator.UI"
             xmlns:viewModels="clr-namespace:TagShelfLocator.UI.ViewModels"
             xmlns:views="clr-namespace:TagShelfLocator.UI.Views"
             Startup="Application_Startup"
             Exit="Application_Exit">
    <Application.Resources>
        <DataTemplate x:Key="InventoryView" DataType="{x:Type viewModels:MainViewModel}">
            <views:InventoryView/>
        </DataTemplate>
    </Application.Resources>
</Application>
